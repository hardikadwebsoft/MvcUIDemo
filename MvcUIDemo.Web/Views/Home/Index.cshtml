@model MvcUIDemo.Web.Models.HomeViewModel

@{
    ViewData["Title"] = "Index";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="">
    <!--[if IE]>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <![endif]-->
    <!-- Title to your website -->
    <title>Agent profile</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="favicon.ico">

    <!-- CSS Files -->
    <link rel="stylesheet" href="~/css/normalize.css">
    <link rel="stylesheet" href="~/css/jquery.formstyler.css">
    <link rel="stylesheet" href="~/css/jquery.formstyler.theme.css">
    <link rel="stylesheet" href="~/css/slick.css">
    <link rel="stylesheet" href="~/css/slick-theme.css">
    <link rel="stylesheet" href="~/css/style.css">

    <!-- Gotham Fonts -->
    <link rel="stylesheet" type="text/css" href="~/css/fonts.css" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!--[if lt IE 9]>
        <div style='clear: both; text-align:center; position: relative;'>
            <a href="http://www.microsoft.com/windows/internet-explorer/default.aspx?ocid=ie6_countdown_bannercode"><img src="http://storage.ie6countdown.com/assets/100/images/banners/warning_bar_0000_us.jpg" border="0" alt="" /></a>
        </div>
    <![endif]-->
</head>
<body>
    <!--==============================Header=================================-->
    <header id="header" class="c-header">
        <div class="l-row l-row--logo h-clearfix">
            <div class="c-header__logo">
                <a href="/">
                    <span class="c-header__logo-icon"></span>
                    <span class="c-header__logo-txt">Houses.Forsale</span>
                </a>
            </div>
            <div class="c-header__login">
                <div class="c-header__login-toggle h-clearfix">
                    <div class="c-header__login-icon">
                        <div class="c-header__login-avatar">
                            <img src="~/img/agents/agent1.png" alt="">
                            <span class="c-header__user-notices c-header__user--has-notices">1</span>
                        </div>
                    </div>
                    <div class="c-header__login-btn">
                        <span class="c-header__login-txt">Login</span>
                        <span class="c-header__login-carrot"></span>
                        <span class="c-header__login-close l-close-btn"></span>
                    </div>
                </div>
                <div class="l-dropdown l-dropdown--login">
                    <form class="l-form l-login-form">
                        <div class="l-form__holder">
                            <div class="l-form__inner">
                                <div class="l-form-row">
                                    <div class="l-form-group">
                                        <input type="email" class="l-form-control" id="inputEmailDropdown" required>
                                        <label for="inputEmailDropdown" class="l-form-label">email</label>
                                        <span class="l-form-control-focus"></span>
                                    </div>
                                </div>
                                <div class="l-form-row">
                                    <div class="l-form-group">
                                        <input type="password" class="l-form-control" id="inputPasswordDropdown" required>
                                        <label for="inputPasswordDropdown" class="l-form-label">password</label>
                                        <span class="l-form-control-focus"></span>
                                        <a href="#" class="l-form-link">Forgot Password?</a>
                                    </div>
                                </div>
                                <div class="l-form-row l-form-row--submit l-row--center">
                                    <button type="submit" class="l-btn l-btn-primary">Sign in</button>
                                    <div class="l-form-text">
                                        <a href="#" class="l-form-link">Create an Account</a>
                                    </div>
                                </div>
                                <div class="l-form-row l-form-row--login-social">
                                    <div class="l-divider l-divider--form l-divider--light"><span>or</span></div>
                                    <div class="l-form-buttons">
                                        <button type="button" class="l-btn l-btn-login l-btn-login--facebook">Sign in with Facebook</button>
                                        <button type="button" class="l-btn l-btn-login l-btn-login--google">Sign in with Google</button>
                                        <button type="button" class="l-btn l-btn-login l-btn-login--microsoft">Sign in with Microsoft</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <ul class="l-dropdown l-dropdown--user-menu">
                    <li class="notices">
                        <a href="#"><span class="c-header__user-notices c-header__user--has-notices">1</span> Notices</a>
                    </li>
                    <li>
                        <a href="#">Account Settings</a>
                    </li>
                    <li>
                        <a href="#">Saved Searches</a>
                    </li>
                    <li>
                        <a href="#">Viewed Homes</a>
                    </li>
                    <li>
                        <a href="#">Favorited Homes</a>
                    </li>
                    <li>
                        <a href="#">Log Out</a>
                    </li>
                </ul>
            </div>
            <div class="c-header__nav">
                <ul class="h-clearfix">
                    <li class="has-dropdown">
                        <span>Buy</span>
                        <ul class="l-dropdown l-dropdown--left">
                            <li class="user-location">
                                84004 - Alpine
                            </li>
                            <li>
                                <a href="#">All Homes</a>
                            </li>
                            <li>
                                <a href="#">New Construction</a>
                            </li>
                            <li>
                                <a href="#">Forclosures</a>
                            </li>
                            <li>
                                <a href="#">Land</a>
                            </li>
                            <li>
                                <a href="#">Recently Sold</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">Sell</a>
                    </li>
                    <li>
                        <a href="#">Neighborhood</a>
                    </li>
                    <li>
                        <a href="#">Mortgage</a>
                    </li>
                    <li class="has-dropdown">
                        <span>Agents</span>
                        <ul class="l-dropdown l-dropdown--right">
                            <li>
                                <a href="#">Looking for an Agent</a>
                            </li>
                            <li>
                                <a href="#">Advertising Solutions</a>
                            </li>
                            <li>
                                <a href="#">Create Agent Profile</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </header>
    <button type="button" class="c-header__nav-toggle">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="c-header__nav-close l-close-btn"></span>
    </button>
    <!--==============================Content================================-->
    <div id="content" class="c-content">
        <div class="c-content__wrap">
            <div class="l-container">
                <div class="c-content__agent-profile h-clearfix">
                    @if (Model != null && Model.AgentProfile != null)
                    {
                        <div class="c-content__agent-overview l-col l-col-8">
                            <div class="c-content__agent-card">
                                <h1>@Model.AgentProfile.AgentFirstname @Model.AgentProfile.AgentLastname</h1>
                                <div class="c-content__agent-profile-photo">
                                    <img src="@Model.AgentProfile.AgentProfPictPath" alt="img">
                                </div>
                                @if (!string.IsNullOrEmpty(Model.AgentProfile.Ratings))
                                {
                                    <div class="c-content__rating c-content__rating--agent">
                                        <div class="c-content__rating-stars">
                                            @for (var i = 0; i < Model.AgentProfile.IntRatings; i++)
                                            {
                                                <span class="c-content__rating-star c-content__rating-star--full"></span>
                                            }
                                            @if (Model.AgentProfile.IsDecimal)
                                            {
                                                <span class="c-content__rating-star c-content__rating-star--half-full"></span>
                                            }
                                        </div>
                                        <div class="c-content__rating-info">
                                            <div class="c-content__rating-average">@Model.AgentProfile.Ratings</div>
                                            <a href="#" class="c-content__agent-review-count">@Model.AgentProfile.ReviewCount</a>
                                        </div>
                                    </div>
                                }

                                @if (Model != null && Model.AgentProfile != null)
                                {
                                    <div class="c-content__skill-bars c-content__skill-bars--agent">
                                        @if (!string.IsNullOrEmpty(Model.AgentProfile.LocalknowledgeRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Local knowledge</div>
                                                <div class="c-content__skill-bar-average">@Model.AgentProfile.LocalknowledgeRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:90%;"></div>
                                                </div>
                                            </div>
                                        }

                                        @if (!string.IsNullOrEmpty(Model.AgentProfile.ProcessexpertiseRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Process expertise</div>
                                                <div class="c-content__skill-bar-average">@Model.AgentProfile.ProcessexpertiseRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:84%;"></div>
                                                </div>
                                            </div>
                                        }

                                        @if (!string.IsNullOrEmpty(Model.AgentProfile.ResponsivenessRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Responsiveness</div>
                                                <div class="c-content__skill-bar-average">@Model.AgentProfile.ResponsivenessRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:100%;"></div>
                                                </div>
                                            </div>
                                        }

                                        @if (!string.IsNullOrEmpty(Model.AgentProfile.NegotiationskillsRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Negotiation Skills</div>
                                                <div class="c-content__skill-bar-average">@Model.AgentProfile.NegotiationskillsRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:82%;"></div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                }

                            </div>
                            <div class="c-content__agent-desc">
                                @if (!string.IsNullOrEmpty(Model.AgentProfile.AboutAgent))
                                {
                                    <h4 class="l-border">About</h4>
                                    <p>
                                        @Model.AgentProfile.AboutAgent
                                    </p>
                                }
                                @if (!string.IsNullOrEmpty(Model.AgentProfile.AgentObjectives))
                                {
                                    <p>
                                        @Model.AgentProfile.AgentObjectives
                                    </p>
                                }

                                @if (!string.IsNullOrEmpty(Model.AgentProfile.Brokerage))
                                {
                                    <p class="l-small">
                                        <strong>Brokerage:</strong> @Model.AgentProfile.Brokerage
                                    </p>
                                }

                                @if (!string.IsNullOrEmpty(Model.AgentProfile.Specialties))
                                {
                                    <p class="l-small">
                                        <strong>Specialties:</strong> @Model.AgentProfile.Specialties
                                    </p>
                                }

                                @if (!string.IsNullOrEmpty(Model.AgentProfile.LicenseNumbers))
                                {
                                    <p class="l-small">
                                        <strong>License Number(s):</strong> @Model.AgentProfile.LicenseNumbers
                                    </p>
                                }
                            </div>
                        </div>
                    }

                    <div class="c-content__agent-form l-col l-col-4">
                        <div class="l-box l-box--contact-agent">
                            <form class="l-form l-contact-agent-form">
                                <div class="l-form__holder">
                                    <h5>Contact Melissa</h5>
                                    <div class="l-form__inner">
                                        <div class="l-form-row h-clearfix">
                                            <div class="l-form-group">
                                                <input type="text" class="l-form-control" id="inputFirstName_ContactAgentForm" required>
                                                <label for="inputFirstName_ContactAgentForm" class="l-form-label">first name</label>
                                                <span class="l-form-control-focus"></span>
                                            </div>
                                        </div>
                                        <div class="l-form-row h-clearfix">
                                            <div class="l-form-group">
                                                <input type="text" class="l-form-control" id="inputLastName_ContactAgentForm" required>
                                                <label for="inputLastName_ContactAgentForm" class="l-form-label">last name</label>
                                                <span class="l-form-control-focus"></span>
                                            </div>
                                        </div>
                                        <div class="l-form-extra demo">
                                            <div class="l-form-row h-clearfix">
                                                <div class="l-form-group">
                                                    <input type="email" class="l-form-control" id="inputEmail_ContactAgentForm" required>
                                                    <label for="inputEmail_ContactAgentForm" class="l-form-label">email</label>
                                                    <span class="l-form-control-focus"></span>
                                                </div>
                                            </div>
                                            <div class="l-form-row h-clearfix">
                                                <div class="l-form-group">
                                                    <input type="text" class="l-form-control" id="inputPhone_ContactAgentForm" required>
                                                    <label for="inputPhone_ContactAgentForm" class="l-form-label">phone</label>
                                                    <span class="l-form-control-focus"></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="l-form-row h-clearfix">
                                            <div class="l-form-group l-form-group--textarea">
                                                <textarea class="l-form-control" id="areaComment_ContactAgentForm" required></textarea>
                                                <label for="areaComment_ContactAgentForm" class="l-form-label">comment</label>
                                                <span class="l-form-control-focus"></span>
                                            </div>
                                        </div>
                                        <div class="l-form-row l-form-row--submit l-row--center">
                                            <button type="submit" class="l-btn l-btn-primary">Submit</button>
                                            <div class="l-form__buttons h-clearfix">
                                                <a href="#" class="l-btn l-btn--action l-btn--gray">Call</a>
                                                <a href="#" class="l-btn l-btn--action l-btn--gray">Leave Review</a>
                                            </div>
                                            <div class="l-form-text">
                                                <a href="#">www.crosbyrealestate.com</a>
                                            </div>
                                            <ul class="l-list l-list--social c-content__agent-form-social">
                                                <li>
                                                    <a href="#" target="_blank">
                                                        <span class="l-list--social__icon">
                                                            <img src="~/img/social/icon-twitter-agent.svg" alt="">
                                                        </span>
                                                        <span class="l-list--social__icon--hover">
                                                            <img src="~/img/social/icon-twitter-agent-dark.svg" alt="">
                                                        </span>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#" target="_blank">
                                                        <span class="l-list--social__icon">
                                                            <img src="~/img/social/icon-facebook-agent.svg" alt="">
                                                        </span>
                                                        <span class="l-list--social__icon--hover">
                                                            <img src="~/img/social/icon-facebook-agent-dark.svg" alt="">
                                                        </span>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#" target="_blank">
                                                        <span class="l-list--social__icon">
                                                            <img src="~/img/social/icon-linkedin-agent.svg" alt="">
                                                        </span>
                                                        <span class="l-list--social__icon--hover">
                                                            <img src="~/img/social/icon-linkedin-agent-dark.svg" alt="">
                                                        </span>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="l-divider l-divider--big"></div>
                @if (Model != null && Model.PropertyList != null && Model.PropertyList.Any())
                {
                    <h3>Active Listings</h3>
                    <div class="c-content__property c-content__property--related">

                        @foreach (var property in Model.PropertyList)
                        {
                            string isFav = "is-favorite";
                            if (!property.IsFavorite) { isFav = ""; }
                            <div class="c-content__property-item @isFav">
                                <a href="#">
                                    <div class="c-content__property-main">
                                        <div class="c-content__property-image">
                                            <img src="@property.PropertyImg" alt="">
                                        </div>
                                        <div class="c-content__property-price h-clearfix">
                                            <div class="c-content__property-price-current">
                                                @property.Price
                                            </div>
                                            @if (!property.IsPriceDown)
                                            {
                                                @if (!string.IsNullOrEmpty(property.PriceUpDown))
                                                {
                                                    <div class="c-content__property-price-changes c-content__property-price-changes--down">
                                                        <span class="value">@property.PriceUpDown</span>
                                                    </div>
                                                }
                                            }
                                            else
                                            {
                                                @if (!string.IsNullOrEmpty(property.PriceUpDown))
                                                {
                                                    <div class="c-content__property-price-changes c-content__property-price-changes--up">
                                                        <span class="value">@property.PriceUpDown</span>
                                                    </div>
                                                }
                                            }
                                        </div>
                                    </div>
                                    <div class="c-content__property-desc">
                                        <div class="c-content__property-info h-clearfix">
                                            <div class="c-content__property-address">
                                                @property.PropertyAddress
                                            </div>
                                            <div class="c-content__property-photos">
                                                <span>×</span> @property.ImageCount
                                            </div>
                                        </div>
                                        <div class="c-content__property-features h-clearfix">
                                            <div class="c-content__property-feature c-content__property-feature--beds">
                                                @property.NumOfBeds
                                            </div>
                                            <div class="c-content__property-feature c-content__property-feature--bath">
                                                @property.NumOfBaths
                                            </div>
                                            <div class="c-content__property-feature c-content__property-feature--sqft">
                                                @property.Sqft
                                            </div>
                                        </div>
                                    </div>
                                </a>
                                <div class="c-content__property-favorite"></div>
                            </div>
                        }
                    </div>
                    <div class="l-row l-row--center">
                        <a class="l-btn l-btn-more l-btn-more--arrow-changeable" href="#"><span class="h-hide-on-mobile">See </span>More</a>
                    </div>
                }
                <div class="l-divider l-divider--big"></div>
                @if (Model != null && Model.PastSalesList != null && Model.PastSalesList.Any())
                {
                    <h3>Past Sales</h3>
                    <div class="c-content__homes">
                        <div class="c-content__homes-row">
                            <div class="c-content__homes-col c-content__homes-col--property">
                                Property
                            </div>
                            <div class="c-content__homes-col c-content__homes-col--details">
                                Details
                            </div>
                            <div class="c-content__homes-col c-content__homes-col--represented">
                                Represented
                            </div>
                            <div class="c-content__homes-col c-content__homes-col--price">
                                Price
                            </div>
                        </div>

                        @foreach (var sale in Model.PastSalesList)
                        {
                            <div class="c-content__homes-row">
                                <div class="c-content__homes-col c-content__homes-col--property">
                                    <div class="c-content__homes-property-photo">
                                        <img src="@sale.PropertyImg" alt="img">
                                    </div>
                                </div>
                                <div class="c-content__homes-col c-content__homes-col--details">
                                    <span class="c-content__homes-price">@sale.Details.Price</span>
                                    <span class="c-content__homes-location">@sale.Details.Location</span>
                                    <span class="c-content__homes-address">@sale.Details.PropertyAddress</span>
                                    <span class="c-content__homes-info">
                                        @sale.Details.NumOfBeds, @sale.Details.NumOfBaths,
                                        @sale.Details.Sqft
                                    </span>
                                    <span class="c-content__homes-date">Sold date: @sale.Details.SoldDate</span>
                                </div>
                                <div class="c-content__homes-col c-content__homes-col--represented">
                                    @sale.Represented
                                </div>
                                <div class="c-content__homes-col c-content__homes-col--price">
                                    @sale.Details.Price
                                </div>
                            </div>
                        }
                    </div>
                    <ul class="l-list l-list--pagination h-hide-on-tablet">
                        <li class="l-list__pagination-control l-list__pagination-control--prev l-list__pagination-control--disabled">
                            <a href="#"></a>
                        </li>
                        <li class="l-list__pagination-pages">
                            <span class="l-list-pagination__current">1</span>
                            <span class="l-list-pagination__pages-count">32</span>
                        </li>
                        <li class="l-list__pagination-control l-list__pagination-control--next">
                            <a href="#"></a>
                        </li>
                    </ul>
                    <div class="l-row l-row--center h-hide-on-desktop">
                        <a class="l-btn l-btn-more l-btn-more--arrow-down" href="#"><span class="h-hide-on-mobile">See </span>More</a>
                    </div>
                }
                <div class="l-divider l-divider--big"></div>

                @if (Model != null && Model.RatingsAndReviews != null && Model.RatingsAndReviews.CustomerReviewsList != null &&
                   Model.RatingsAndReviews.CustomerReviewsList.Any())
                {
                    var cnt = 0;
                    <h3>Ratings &amp; Reviews</h3>
                    <div class="c-content__filter c-content__filter--reviews h-clearfix">
                        <div class="l-col l-col-4 l-col-xs-6">
                            <select class="l-form-control l-form-select l-form-elem-custom" id="reviewType_ReviewFilter">
                                <option>All reviews (16)</option>
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                        </div>
                        <div class="l-col l-col-4 l-col-xs-6">
                            <select class="l-form-control l-form-select l-form-elem-custom" id="reviewOrder_ReviewFilter">
                                <option>Newest first</option>
                                <option>1</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                        </div>
                        <div class="l-col l-col-4 h-hide-on-mobile">
                            <button type="button" class="l-btn l-btn--action l-btn--blue">Write a Review</button>
                        </div>
                    </div>
                    <div class="c-content__reviews">
                        @foreach (var review in Model.RatingsAndReviews.CustomerReviewsList)
                        {
                            cnt += 1;
                            <div class="c-content__review-item">
                                <div class="c-content__review-summary">
                                    <div class="c-content__review-info">
                                        <div class="c-content__review-date">
                                            @review.Date
                                        </div>
                                        <div class="c-content__review-meta h-show-on-mobile">
                                            <div class="c-content__review-author">
                                                Reviewed by: @review.ReviewedBy
                                            </div>
                                            <div class="c-content__review-home">
                                                @review.ReviewHome
                                            </div>
                                        </div>
                                        @if (!string.IsNullOrEmpty(review.Ratings))
                                        {
                                            <div class="c-content__rating c-content__rating--agent">
                                                <div class="c-content__rating-stars">
                                                    @for (var i = 0; i < review.IntRatings; i++)
                                                    {
                                                        <span class="c-content__rating-star c-content__rating-star--full"></span>
                                                    }
                                                    @if (review.IsDecimal)
                                                    {
                                                        <span class="c-content__rating-star c-content__rating-star--half-full"></span>
                                                    }
                                                </div>
                                                <div class="c-content__rating-info">
                                                    <div class="c-content__rating-average">@review.Ratings</div>
                                                </div>
                                            </div>
                                        }
                                        <div class="c-content__review-meta h-hide-on-mobile">
                                            <div class="c-content__review-author">
                                                Reviewed by: @review.ReviewedBy
                                            </div>
                                            <div class="c-content__review-home">
                                                @review.ReviewHome
                                            </div>
                                        </div>
                                    </div>
                                    <div class="c-content__skill-bars c-content__skill-bars--review">
                                        @if (!string.IsNullOrEmpty(review.LocalknowledgeRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Local knowledge</div>
                                                <div class="c-content__skill-bar-average">@review.LocalknowledgeRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:90%;"></div>
                                                </div>
                                            </div>
                                        }

                                        @if (!string.IsNullOrEmpty(review.ProcessexpertiseRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Process expertise</div>
                                                <div class="c-content__skill-bar-average">@review.ProcessexpertiseRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:84%;"></div>
                                                </div>
                                            </div>
                                        }

                                        @if (!string.IsNullOrEmpty(review.ResponsivenessRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Responsiveness</div>
                                                <div class="c-content__skill-bar-average">@review.ResponsivenessRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:100%;"></div>
                                                </div>
                                            </div>
                                        }

                                        @if (!string.IsNullOrEmpty(review.NegotiationskillsRatings))
                                        {
                                            <div class="c-content__skill-bar">
                                                <div class="c-content__skill-bar-title">Negotiation Skills</div>
                                                <div class="c-content__skill-bar-average">@review.NegotiationskillsRatings</div>
                                                <div class="c-content__skill-bar-row--default">
                                                    <div class="c-content__skill-bar-row" style="width:82%;"></div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                                <div class="c-content__review-content">
                                    @review.ReviewNote 
                                    @if(Model.RatingsAndReviews.CustomerReviewsList.Count == cnt)
                                    {
                                        <a class="l-toggle-content-link" href="#">More</a>
                                    }
                                </div>
                            </div>
                        }
                    </div>
                }
                
                <div class="c-content__copyright c-content__copyright--zillow">
                    <div class="c-content__logo-copyright">
                        <a href="#">Zillow</a>
                    </div>
                    <p>
                        &copy; Zillow, Inc., 2006-2016. Use is subject to <a href="#">Terms of Use</a>
                    </p>
